{
    "name": "mateusjunges/laravel-pt-br-translations",
    "description": "Laravel validation messages in Portuguese",
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mateusjunges",
            "email": "mateus@junges.dev",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Junges\\PtBrTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Junges\\PtBrTranslations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel" : {
            "providers": [
                "Junges\\PtBrTranslations\\PtBrTranslationsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}
